<h1>Send us your tipp!</h1>

<%= form_for @tipp do |f|%>
<%= f.label :name %><br />
<%= f.text_field :name %><br />
<%= f.label :streetname %><br />
<%= f.text_field :streetname %><br />
<%= f.label :user_id %>
<%= f.hidden_field :user_id, :value => current_user.id %> +
<%= f.label :city_id %>
<%= f.hidden_field :city_id, :value => 1 %><br />
<%= f.submit %>
<% end %>

<p id="error"><%= flash[:error] %></p>



