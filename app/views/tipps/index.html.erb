<h1>Welcome to the randomizer:</h1>
<p id="error"><%= flash[:notice] %></p>

  <% if current_user %>
    Hi <%= current_user.name%>, welcome again!
  <% end %>  
    <br />
    <br />
    Here: <%= @random_choices[0] %>

<ul>
  <% @random_choices.each do |tipp| %>
  <li>
    Activity: <%= tipp.name %> (<%= tipp.streetname %>,<%= tipp.city_id %>)
  </li>
  <% end %>
</ul>